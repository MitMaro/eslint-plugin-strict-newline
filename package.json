{
  "name": "eslint-plugin-strict-newline",
  "version": "1.0.0",
  "description": "ESLint plugin that provides a rule for newline spacing around 'use strict'",
  "author": "Tim Oram @MitMaro <mitmaro@gmail.com>",
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MitMaro/eslint-plugin-strict-newline.git"
  },
  "scripts": {
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "lint": "eslint ./",
    "test": "npm run lint && npm run unit-test:coverage",
    "unit-test:coverage": "istanbul cover node_modules/mocha/bin/_mocha",
    "unit-test": "mocha"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "coveralls": "2.11.11",
    "istanbul": "0.4.4",
    "mocha": "2.5.3"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin"
  ],
  "license": "ICS"
}
